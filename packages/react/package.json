{
  "name": "@vimeo-player/react",
  "version": "0.0.0",
  "description": "vimeo player unofficial react wrapper",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "author": "Shubham Singh <hello@shubham.sh>",
  "license": "MIT",
  "sideEffects": false,
  "devDependencies": {
    "@vimeo-player/core": "*",
    "@storybook/react": "^6.3.12"
  },
  "peerDependencies": {
    "@vimeo-player/core": "^0.0.0",
    "react": ">= 16.8.0"
  },
  "homepage": "https://github.com/imshubhamsingh/vimeo-player#readme",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imshubhamsingh/vimeo-player.git"
  },
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build": "rollup --c ./rollup.config.js --environment production",
    "build:dev": "rollup --c ./rollup.config.js --environment development",
    "fix": "pnpx eslint --fix \"src/**/*.{js,svelte}\"",
    "format": "pnpx prettier --write \"src/**/*.{js,svelte}\""
  },
  "bugs": {
    "url": "https://github.com/imshubhamsingh/vimeo-player/issues"
  }
}
